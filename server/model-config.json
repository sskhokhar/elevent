{
    "_meta": {
        "sources": [
            "loopback/common/models",
            "loopback/server/models",
            "../common/models",
            "./models"
        ],
        "mixins": [
            "loopback/common/mixins",
            "loopback/server/mixins",
            "../common/mixins",
            "./mixins"
        ]
    },
    "User": {
        "dataSource": "mongoDb",
        "public": false
    },
    "AccessToken": {
        "dataSource": "mongoDb",
        "public": true
    },
    "ACL": {
        "dataSource": "mongoDb",
        "public": true
    },
    "RoleMapping": {
        "dataSource": "mongoDb",
        "public": true,
        "options": {
            "strictObjectIDCoercion": true
        }
    },
    "Role": {
        "dataSource": "mongoDb",
        "public": true
    },
    "Eventmanager": {
        "dataSource": "mongoDb",
        "public": true
    },
    "Event": {
        "dataSource": "mongoDb",
        "public": true
    },
    "Task": {
        "dataSource": "mongoDb",
        "public": true
    },
    "Vendor": {
        "dataSource": "mongoDb",
        "public": true
    }
}