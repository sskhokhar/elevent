{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongoDb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDb",
    "public": false
  },
  "Eventmanager": {
    "dataSource": "mongoDb",
    "public": true
  },
  "Event": {
    "dataSource": "mongoDb",
    "public": true
  },
  "Task": {
    "dataSource": "mongoDb",
    "public": true
  },
  "Vendor": {
    "dataSource": "mongoDb",
    "public": true
  },
  "Attachment": {
    "dataSource": "FileStorage",
    "public": true
  },
  "email": {
    "dataSource": "EmailDatasource",
    "public": true
  },
  "Collaborator": {
    "dataSource": "mongoDb",
    "public": true
  }
}
