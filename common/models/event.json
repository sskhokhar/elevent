{
    "name": "Event",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "title": {
            "type": "string"
        },
        "date": {
            "type": "date"
        },
        "guests": {
            "type": "number"
        },
        "venue": {
            "type": "string"
        },
        "image": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "collaboratorIds": {
            "type": [
                "string"
            ]
        }
    },
    "validations": [],
    "relations": {
        "eventManagerId": {
            "type": "belongsTo",
            "model": "Eventmanager",
            "foreignKey": "eventManagerId"
        },
        "tasks": {
            "type": "referencesMany",
            "model": "Task",
            "foreignKey": "taskIds",
            "options": {
                "validate": true,
                "forceId": false
            }
        },
        "vendor": {
            "type": "referencesMany",
            "model": "Vendor",
            "foreignKey": "vendorIds",
            "options": {
                "validate": true,
                "forceId": false
            }
        }
    },
    "acls": [{
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "superUser",
            "permission": "ALLOW"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW"
        },
        {
            "accessType": "EXECUTE",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "create"
        }
    ],
    "methods": {}
}