{
    "name": "Event",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "location": {
            "type": "string"
        }
    },
    "validations": [],
    "relations": {
        "userId": {
            "type": "belongsTo",
            "model": "Eventmanager",
            "foreignKey": "eventManagerId"
        },
        "tasks": {
            "type": "referencesMany",
            "model": "Task",
            "foreignKey": "taskIds",
            "options": {
                "validate": true,
                "forceId": false
            }
        }
    },
    "acls": [],
    "methods": {}
}