{
  "name": "Task",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "description": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "isDone": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "event": {
      "type": "belongsTo",
      "model": "Event",
      "foreignKey": ""
    },
    "vendor": {
      "type": "referencesMany",
      "model": "Vendor",
      "foreignKey": "vendorIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superUser",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
